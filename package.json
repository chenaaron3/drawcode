{
  "name": "python-quest",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NEXT_FORCE_WEBPACK=1 next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "NEXT_FORCE_WEBPACK=1 next dev -p ${PORT:-3000}",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "trace": "python3 ./src/tracer/trace.py",
    "lesson": "python3 ./scripts/create_lesson_template.py",
    "gen-lesson": "python3 ./scripts/generate_lesson.py ./scripts/lesson.txt"
  },
